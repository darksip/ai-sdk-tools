{
  "name": "@fondation-io/devtools",
  "version": "1.0.0",
  "description": "AI development tools for debugging and monitoring AI SDKs streams",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "devtools",
    "debugging",
    "chat",
    "streaming",
    "development",
    "ai-sdk",
    "react"
  ],
  "author": "Pontus Abrahamsson",
  "license": "MIT",
  "peerDependencies": {
    "@ai-sdk/react": ">=0.0.1",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@fondation-io/store": "*"
  },
  "peerDependenciesMeta": {
    "@fondation-io/store": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "@ai-sdk/react": "^2.0.71",
    "@fondation-io/store": "workspace:*"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@types/dagre": "^0.7.53",
    "@xyflow/react": "^12.8.6",
    "ai": "^5.0.71",
    "dagre": "^0.8.5",
    "react-json-view-lite": "^2.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darksip/ai-sdk-tools.git"
  },
  "bugs": {
    "url": "https://github.com/darksip/ai-sdk-tools/issues"
  },
  "homepage": "https://github.com/darksip/ai-sdk-tools#readme",
  "publishConfig": {
    "access": "public"
  }
}
