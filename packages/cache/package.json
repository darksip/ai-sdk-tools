{
  "name": "@fondation-io/cache",
  "version": "1.0.0",
  "description": "Simple caching wrapper for AI SDK tools - cache expensive tool executions with zero configuration",
  "keywords": [
    "ai",
    "ai-sdk",
    "tools",
    "cache",
    "caching",
    "performance",
    "llm",
    "openai",
    "anthropic"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/darksip/ai-sdk-tools.git",
    "directory": "packages/cache"
  },
  "bugs": {
    "url": "https://github.com/darksip/ai-sdk-tools/issues"
  },
  "homepage": "https://github.com/darksip/ai-sdk-tools/tree/main/packages/cache#readme",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "ai": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@fondation-io/artifacts": {
      "optional": true
    }
  },
  "devDependencies": {
    "@fondation-io/artifacts": "workspace:*",
    "@types/node": "^24.7.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "zod": "^4.1.12",
    "ai": "^5.0.71"
  },
  "publishConfig": {
    "access": "public"
  }
}
